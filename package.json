{
  "name": "cibulb",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:screendriver/cibulb.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)' 'typings/**/*.ts'",
    "test:unit": "nyc tape -r source-map-support/register 'dist/test/unit/**/*.js'",
    "test:integration": "tape -r source-map-support/register 'dist/test/integration/**/*.js' | tap-dot",
    "test": "yarn lint && yarn prettier:check && yarn test:unit | tap-dot && yarn test:integration",
    "test:ci": "yarn lint && yarn prettier:check && yarn test:unit | tap-xunit > test-results.xml && yarn test:integration",
    "coverage": "nyc report --reporter=text-summary",
    "build": "tsc --build"
  },
  "dependencies": {
    "@sentry/node": "5.0.5",
    "got": "9.6.0",
    "loglevel": "1.6.1",
    "micro": "9.3.3",
    "mongodb": "3.1.13",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/got": "9.4.1",
    "@types/loglevel": "1.5.4",
    "@types/micro": "7.3.3",
    "@types/mongodb": "3.1.22",
    "@types/mongodb-memory-server": "1.8.0",
    "@types/node": "8.10.45",
    "@types/sinon": "7.0.11",
    "@types/tape": "4.2.33",
    "@types/test-listen": "1.1.0",
    "mongodb-memory-server": "4.2.2",
    "now": "15.0.4",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "sinon": "7.3.1",
    "source-map-support": "0.5.12",
    "tap-dot": "2.0.0",
    "tap-xunit": "2.3.0",
    "tape": "4.10.1",
    "test-listen": "1.1.0",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.3",
    "typescript-tslint-plugin": "0.3.1"
  }
}
